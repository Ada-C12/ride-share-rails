<section class="heading">
<h1>Passengers</h1> <%= button_to "Add New Passenger", new_passenger_path, :method => "get" %>
</section>
<div class="wrapper_people">
  <% @passengers.each do |passenger|  %>
    <div><%= passenger.id %>:  <%= link_to passenger.name, passenger_path(passenger) %></div>
  <% end %>
</div>