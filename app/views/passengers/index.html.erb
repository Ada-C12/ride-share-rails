<section>
  <h2>Passengers</h2>

<table class="table">
  <thead>
    <tr>
      <th>Passenger Name</th>
      <th>Phone Number</th>
    </tr>
  </thead>
  <tbody>
    <% @passengers.each do |passenger| %>
      <tr>
        <td> <%= link_to passenger.name, passenger_path(passenger.id) %> </td>
        <td> <%= passenger.phone_num %> </td>
      </tr>
    <% end %>
  </tbody>

</section>