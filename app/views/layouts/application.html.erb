<!DOCTYPE html>
<html>
  <head>
    <title>RideShareRails</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <header>
      <h1>Welcome to Ada RideShareRails</h1>
    </header>
    <div>
      <ul>
        <li>
          <%= link_to "New Driver", new_driver_path %>
        </li>
        <li>
           <%= link_to "New Passenger", new_passenger_path %>
        </li>
    </div>
    <%= yield %>
  </body> 
</html>
