<!DOCTYPE html>
<html>
  <head>
    <title>RideShareRails</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

    <h1 >Rideshare Rails!</h1>
    <ul>
      <li><%= link_to "Passengers", passengers_path%></li>
      <li><%= link_to "Drivers", drivers_path%></li>
      <li><%= link_to "Add Driver", new_driver_path%></li>
      <li><%= link_to "Add Passenger", new_passenger_path%></li>
    </ul>
  <body>
    <%= yield %>
  </body>
</html>
